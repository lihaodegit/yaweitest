export default {
  namespaced: true,
  state: {
    id: 0,
    name: '',
    orgName:''
  },
  mutations: {
    updateId (state, id) {
      state.id = id
    },
    updateName (state, name) {
      state.name = name
    },
    updateOrgName (state, orgname) {
      state.orgName = orgname
    }

  }
}
